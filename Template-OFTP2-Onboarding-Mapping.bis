(: ============================================================================
   BIS Mapping: Template-OFTP2 Onboarding Mapping

   Purpose: Transforms Template-OFTP2.xml using getInputParameter("participantid")
            for dynamic values while keeping fixed IDs for records 6-8

   Source: Template-OFTP2.xml
   Created: 2026-02-05

   Input Parameters:
   - participantid: The partner identifier used for naming
   - mode: "create" (default) generates new UUIDs, "update" preserves existing IDs

   Records Structure:
   1. oftp.oftp-partner - Uses participantid for name
   2. oftp.sfid-partner - Uses participantid for name, certificate refs
   3. ksm-entry - Partner auth cert ({participantid}-partnerauth)
   4. ksm-entry - Partner EERP cert ({participantid}-partnereerp)
   5. ksm-entry - Partner encrypt cert ({participantid}-partnerencrypt)
   6. ksm-entry - Demoim private key (FIXED: a8e279ca-cfa9-11ec-8563-ea550a0b1393)
   7. oftp.oftp-listener - Listener (FIXED: 26fcb4c0-37b1-11f0-8e28-c61e7f000101)
   8. oftp.sfid-personality - SFID personality (FIXED: 273f6310-37b1-11f0-8e28-c61e7f000101)
   ============================================================================ :)

(: Input parameters :)
let $participantid := getInputParameter("participantid")
let $mode := getInputParameter("mode")

(: Conditional ID generation: "update" mode preserves existing IDs, otherwise generate new UUIDs :)
(: Use type-based predicates since record order is not fixed :)
let $oftp_partner_id := if ($mode = "update") then /transport/records/record[@type="oftp.oftp-partner"]/@rest-identifier else generateUUID()
let $sfid_partner_id := if ($mode = "update") then /transport/records/record[@type="oftp.sfid-partner"]/@rest-identifier else generateUUID()
let $partner_auth_cert_id := if ($mode = "update") then /transport/records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/@rest-identifier else generateUUID()
let $partner_eerp_cert_id := if ($mode = "update") then /transport/records/record[@type="ksm-entry" and contains(@name, "-partnereerp")]/@rest-identifier else generateUUID()
let $partner_encrypt_cert_id := if ($mode = "update") then /transport/records/record[@type="ksm-entry" and contains(@name, "-partnerencrypt")]/@rest-identifier else generateUUID()

(: Certificate aliases based on participantid :)
let $partner_auth_cert_alias := concat($participantid, "-partnerauth")
let $partner_eerp_cert_alias := concat($participantid, "-partnereerp")
let $partner_encrypt_cert_alias := concat($participantid, "-partnerencrypt")

(: Root transport element - use /transport context :)
transport <- /transport
{
    (: Transport ID uses participantid :)
    @id := $participantid
    @format := @format
    @security-level := @security-level

    (: Administrative data with all attributes from source :)
    administrative-data
    {
        @created-at := administrative-data/@created-at
        @created-by := administrative-data/@created-by
        @changed-at := administrative-data/@changed-at
        @changed-by := administrative-data/@changed-by
        @logical-system := administrative-data/@logical-system
    }

    (: Simple root-level element mappings :)
    direction := direction
    state := state
    processing := processing
    version := version
    revised-at := revised-at
    revised-by := revised-by

    (: Records collection :)
    records
    {
        (: ================================================================
           RECORD 1: OFTP Partner - Uses participantid for dynamic values
           ================================================================ :)
        record
        {
            @name := $participantid
            @type := "oftp.oftp-partner"
            @rest-identifier := $oftp_partner_id
            @action := records/record[@type="oftp.oftp-partner"]/@action
            @created-at := records/record[@type="oftp.oftp-partner"]/@created-at
            @created-by := records/record[@type="oftp.oftp-partner"]/@created-by
            @last-modified-by := records/record[@type="oftp.oftp-partner"]/@last-modified-by
            @last-modified-at := records/record[@type="oftp.oftp-partner"]/@last-modified-at

            oftp-v2-partner
            {
                @identifier := $oftp_partner_id
                name := $participantid

                administrative-data
                {
                    @changed-at := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/administrative-data/@changed-at
                    @changed-by := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/administrative-data/@changed-by
                    @comment := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/administrative-data/@comment
                    @created-at := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/administrative-data/@created-at
                    @created-by := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/administrative-data/@created-by
                    @logical-system := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/administrative-data/@logical-system
                    @record-id := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/administrative-data/@record-id
                }

                active := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/active

                ssid
                {
                    ssid-personality
                    {
                        name := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/ssid/ssid-personality/name
                        odette-code := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/ssid/ssid-personality/odette-code
                        password := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/ssid/ssid-personality/password
                        session-authentication-certificates { }
                    }

                    ssid-partner
                    {
                        name := $participantid
                        odette-code := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/ssid/ssid-partner/odette-code
                        password := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/ssid/ssid-partner/password
                        session-authentication-certificates { }
                    }

                    session-configuration
                    {
                        protocol-version := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/ssid/session-configuration/protocol-version
                        transfer-direction := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/ssid/session-configuration/transfer-direction
                        enable-multiple-sessions := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/ssid/session-configuration/enable-multiple-sessions
                        session-timeout := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/ssid/session-configuration/session-timeout
                        call-timeout := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/ssid/session-configuration/call-timeout
                        buffer-size := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/ssid/session-configuration/buffer-size
                        acknowledgement-interval := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/ssid/session-configuration/acknowledgement-interval
                        enable-compression := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/ssid/session-configuration/enable-compression
                        enable-restart := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/ssid/session-configuration/enable-restart
                        character-encoding := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/ssid/session-configuration/character-encoding
                        request-secure-authentication := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/ssid/session-configuration/request-secure-authentication

                        sequence-processing
                        {
                            use-sequence := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/ssid/session-configuration/sequence-processing/use-sequence
                        }

                        asynchronous-incoming-files-processing
                        {
                            enable-asynchronous-processing := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/ssid/session-configuration/asynchronous-incoming-files-processing/enable-asynchronous-processing
                            minimum-file-size := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/ssid/session-configuration/asynchronous-incoming-files-processing/minimum-file-size
                        }
                    }
                }

                sfid
                {
                    sfid-partners
                    {
                        sfid-partner
                        {
                            sfid-partner-ref { @href := concat("https://dedcdbdmgr01:14000/api/oftp/v2/sfid-partners/", $sfid_partner_id) }
                            name := $participantid
                            odette-code := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/sfid/sfid-partners/sfid-partner/odette-code
                        }
                    }

                    sfid-personalities
                    {
                        sfid-personality
                        {
                            (: Fixed SFID personality reference :)
                            sfid-personality-ref { @href := "https://dedcdbdmgr01:14000/api/oftp/v2/sfid-personalities/273f6310-37b1-11f0-8e28-c61e7f000101" }
                            name := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/sfid/sfid-personalities/sfid-personality/name
                            odette-code := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/sfid/sfid-personalities/sfid-personality/odette-code
                        }
                    }
                }

                partner-tcp-configuration
                {
                    hostname := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/partner-tcp-configuration/hostname
                    port := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/partner-tcp-configuration/port
                    use-tls := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/partner-tcp-configuration/use-tls

                    tls
                    {
                        trust-model := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/partner-tcp-configuration/tls/trust-model
                        use-system-truststore := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/partner-tcp-configuration/tls/use-system-truststore
                        disable-hostname-check := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/partner-tcp-configuration/tls/disable-hostname-check
                        server-certificates { }
                        security-profile { @href := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/partner-tcp-configuration/tls/security-profile/@href }
                        use-client-authentication := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/partner-tcp-configuration/tls/use-client-authentication
                    }

                    check-tls := records/record[@type="oftp.oftp-partner"]/oftp-v2-partner/partner-tcp-configuration/check-tls
                }

                listeners
                {
                    (: Fixed listener reference :)
                    listener { @href := "https://dedcdbdmgr01:14000/api/oftp/listeners/26fcb4c0-37b1-11f0-8e28-c61e7f000101" }
                }
            }
        }

        (: ================================================================
           RECORD 2: SFID Partner - Uses participantid and dynamic cert refs
           ================================================================ :)
        record
        {
            @name := $participantid
            @type := "oftp.sfid-partner"
            @rest-identifier := $sfid_partner_id
            @action := records/record[@type="oftp.sfid-partner"]/@action
            @created-at := records/record[@type="oftp.sfid-partner"]/@created-at
            @created-by := records/record[@type="oftp.sfid-partner"]/@created-by
            @last-modified-by := records/record[@type="oftp.sfid-partner"]/@last-modified-by
            @last-modified-at := records/record[@type="oftp.sfid-partner"]/@last-modified-at

            sfid-partner
            {
                @identifier := $sfid_partner_id
                name := $participantid

                administrative-data
                {
                    @changed-at := records/record[@type="oftp.sfid-partner"]/sfid-partner/administrative-data/@changed-at
                    @changed-by := records/record[@type="oftp.sfid-partner"]/sfid-partner/administrative-data/@changed-by
                    @comment := records/record[@type="oftp.sfid-partner"]/sfid-partner/administrative-data/@comment
                    @created-at := records/record[@type="oftp.sfid-partner"]/sfid-partner/administrative-data/@created-at
                    @created-by := records/record[@type="oftp.sfid-partner"]/sfid-partner/administrative-data/@created-by
                    @logical-system := records/record[@type="oftp.sfid-partner"]/sfid-partner/administrative-data/@logical-system
                    @record-id := records/record[@type="oftp.sfid-partner"]/sfid-partner/administrative-data/@record-id
                }

                odette-code := records/record[@type="oftp.sfid-partner"]/sfid-partner/odette-code
                sfid-user-field { }
                clearing-center := records/record[@type="oftp.sfid-partner"]/sfid-partner/clearing-center

                authentication-certificates
                {
                    authentication-certificate
                    {
                        (: Dynamic certificate reference using participantid :)
                        certificate-ref { @href := concat("https://dedcdbdmgr01:14000/api/ksm/TRUSTED/TRUSTED_INSTANCES/", $partner_auth_cert_alias) }
                    }
                }

                encryption-certificates
                {
                    encryption-certificate
                    {
                        @effective-from-date-time := records/record[@type="oftp.sfid-partner"]/sfid-partner/encryption-certificates/encryption-certificate/@effective-from-date-time
                        (: Dynamic certificate reference using participantid :)
                        certificate-ref { @href := concat("https://dedcdbdmgr01:14000/api/ksm/TRUSTED/TRUSTED_INSTANCES/", $partner_encrypt_cert_alias) }
                    }
                }

                eerp-authentication-certificates
                {
                    eerp-authentication-certificate
                    {
                        (: Dynamic certificate reference using participantid :)
                        certificate-ref { @href := concat("https://dedcdbdmgr01:14000/api/ksm/TRUSTED/TRUSTED_INSTANCES/", $partner_eerp_cert_alias) }
                    }
                }

                send
                {
                    sign := records/record[@type="oftp.sfid-partner"]/sfid-partner/send/sign
                    signature-algorithm := records/record[@type="oftp.sfid-partner"]/sfid-partner/send/signature-algorithm
                    encrypt := records/record[@type="oftp.sfid-partner"]/sfid-partner/send/encrypt
                    encryption-algorithm := records/record[@type="oftp.sfid-partner"]/sfid-partner/send/encryption-algorithm
                    compress := records/record[@type="oftp.sfid-partner"]/sfid-partner/send/compress
                    send-eerp-immediately := records/record[@type="oftp.sfid-partner"]/sfid-partner/send/send-eerp-immediately
                    request-signed-eerp := records/record[@type="oftp.sfid-partner"]/sfid-partner/send/request-signed-eerp
                    disable-eerp-sending := records/record[@type="oftp.sfid-partner"]/sfid-partner/send/disable-eerp-sending
                    disable-eerp-sending-in-receiver-only-mode := records/record[@type="oftp.sfid-partner"]/sfid-partner/send/disable-eerp-sending-in-receiver-only-mode
                    eerp-user-field { }
                    report-queue := records/record[@type="oftp.sfid-partner"]/sfid-partner/send/report-queue
                    cipher-suite := records/record[@type="oftp.sfid-partner"]/sfid-partner/send/cipher-suite
                }

                receive
                {
                    disable-check-for-incoming-transfer-duplicates := records/record[@type="oftp.sfid-partner"]/sfid-partner/receive/disable-check-for-incoming-transfer-duplicates
                    format-received-fix-files := records/record[@type="oftp.sfid-partner"]/sfid-partner/receive/format-received-fix-files
                    accept-only-signed-files := records/record[@type="oftp.sfid-partner"]/sfid-partner/receive/accept-only-signed-files
                    accept-only-encrypted-files := records/record[@type="oftp.sfid-partner"]/sfid-partner/receive/accept-only-encrypted-files
                    partner-sends-eerp := records/record[@type="oftp.sfid-partner"]/sfid-partner/receive/partner-sends-eerp
                    eerp-timeout := records/record[@type="oftp.sfid-partner"]/sfid-partner/receive/eerp-timeout
                    eerp-timeout-unit := records/record[@type="oftp.sfid-partner"]/sfid-partner/receive/eerp-timeout-unit
                    correlate-eerps-in-process-engine := records/record[@type="oftp.sfid-partner"]/sfid-partner/receive/correlate-eerps-in-process-engine
                    accept-unsigned-eerp := records/record[@type="oftp.sfid-partner"]/sfid-partner/receive/accept-unsigned-eerp
                }
            }
        }

        (: ================================================================
           RECORD 3: Partner Auth Certificate - Uses participantid for alias
           ================================================================ :)
        record
        {
            @name := $partner_auth_cert_alias
            @type := "ksm-entry"
            @rest-identifier := $partner_auth_cert_id
            @action := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/@action
            @created-at := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/@created-at
            @created-by := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/@created-by
            @last-modified-by := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/@last-modified-by
            @last-modified-at := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/@last-modified-at

            keystore-entry
            {
                @identifier := $partner_auth_cert_id

                administrative-data
                {
                    @changed-at := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/administrative-data/@changed-at
                    @changed-by := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/administrative-data/@changed-by
                    @created-at := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/administrative-data/@created-at
                    @created-by := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/administrative-data/@created-by
                    @logical-system := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/administrative-data/@logical-system
                    @record-id := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/administrative-data/@record-id
                }

                type := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/type
                alias := $partner_auth_cert_alias
                path := "TRUSTED\\TRUSTED_INSTANCES"
                version := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/version
                subject := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/subject
                issuer := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/issuer
                serial := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/serial
                serial-hex := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/serial-hex
                valid-from := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/valid-from
                valid-to := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/valid-to
                fingerprint := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/fingerprint
                fingerprint-algorithm := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/fingerprint-algorithm

                subject-details
                {
                    common-name := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/subject-details/common-name
                }

                key-settings
                {
                    sign-algorithm := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/key-settings/sign-algorithm
                    public-key-size := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/key-settings/public-key-size
                    private-key-algorithm := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/key-settings/private-key-algorithm
                }

                extensions
                {
                    key-usage
                    {
                        digital-signature := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/extensions/key-usage/digital-signature
                        data-encipherment := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/extensions/key-usage/data-encipherment
                        non-repudation := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/extensions/key-usage/non-repudation
                        verify-certificate-signatures := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/extensions/key-usage/verify-certificate-signatures
                        key-encipherment := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/extensions/key-usage/key-encipherment
                        server-authentication := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/extensions/key-usage/server-authentication
                        client-authentication := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/extensions/key-usage/client-authentication
                        email-protection := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/extensions/key-usage/email-protection
                        key-agreement := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/extensions/key-usage/key-agreement
                        encipher-only := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/extensions/key-usage/encipher-only
                        decipher-only := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/extensions/key-usage/decipher-only
                        verify-crl-signatures := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/extensions/key-usage/verify-crl-signatures
                    }
                    basic-constraint := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/extensions/basic-constraint
                    ca-path-length-constraint := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/extensions/ca-path-length-constraint
                    alternative-subject-names := ""
                }

                raw-content
                {
                    payload := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/raw-content/payload
                }

                validation
                {
                    validation-settings
                    {
                        period := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/validation/validation-settings/period
                        chain := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/validation/validation-settings/chain
                        ocsp := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/validation/validation-settings/ocsp
                        ocsp-online := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/validation/validation-settings/ocsp-online
                        crl := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/validation/validation-settings/crl
                        force-valid := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/validation/validation-settings/force-valid
                    }
                    validity-status := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/validation/validity-status
                    validation-time := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/validation/validation-time
                }

                validation-state := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/validation-state
                provided-password := records/record[@type="ksm-entry" and contains(@name, "-partnerauth")]/keystore-entry/provided-password
            }
        }

        (: ================================================================
           RECORD 4: Partner EERP Certificate - Uses participantid for alias
           ================================================================ :)
        record
        {
            @name := $partner_eerp_cert_alias
            @type := "ksm-entry"
            @rest-identifier := $partner_eerp_cert_id
            @action := records/record[@type="ksm-entry" and contains(@name, "-partnereerp")]/@action
            @created-at := records/record[@type="ksm-entry" and contains(@name, "-partnereerp")]/@created-at
            @created-by := records/record[@type="ksm-entry" and contains(@name, "-partnereerp")]/@created-by
            @last-modified-by := records/record[@type="ksm-entry" and contains(@name, "-partnereerp")]/@last-modified-by
            @last-modified-at := records/record[@type="ksm-entry" and contains(@name, "-partnereerp")]/@last-modified-at

            keystore-entry
            {
                @identifier := $partner_eerp_cert_id

                administrative-data
                {
                    @changed-at := records/record[@type="ksm-entry" and contains(@name, "-partnereerp")]/keystore-entry/administrative-data/@changed-at
                    @changed-by := records/record[@type="ksm-entry" and contains(@name, "-partnereerp")]/keystore-entry/administrative-data/@changed-by
                    @created-at := records/record[@type="ksm-entry" and contains(@name, "-partnereerp")]/keystore-entry/administrative-data/@created-at
